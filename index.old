  <div class="container">
    <div class="row justify-content-center">
      <div>
        <iframe width="600" height="371" seamless frameborder="0" scrolling="no" align="middle" src="https://docs.google.com/spreadsheets/d/e/2PACX-1vRtB3BBIjT8mZapIE2aoOeniqze_k5ahIiNRdd5I4y_zusdDfLNj9q9eySg-mRdby6ZMvV992pcogqH/pubchart?oid=561350747&amp;format=interactive"></iframe>
      </div>
    </div>
  </div>
    
  <div class="row justify-content-center">
      <h2 class="text-center">Tareas</h2>
  </div>
  
  <div class="row">
    <div class="col-sm">
      <ul id="task0" class="list-group">
      </ul>
    </div>
    <div class="col-sm">
      <ul id="task1" class="list-group">
      </ul>
    </div>
    <div class="col-sm">
      <ul id="task2" class="list-group">
      </ul>
    </div>
  </div>
  
  </div>
  

<script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
  <script>
  $(document).ready(function(){
    $.getJSON("https://script.google.com/macros/s/AKfycbyrE19YaL1gkw5otvvV3cGVV2zpjUu1ng1GxHjFSfRhI7vZ7qBJ/exec", function(result){
      var col = 0;
      $.each(result, function(i, field){
        
        if (col==0){
        $("#task0").append("<li class='list-group-item'> " +field + "</li> ");}
        if (col==1){
        $("#task1").append("<li class='list-group-item'> " + field + "</li> ");}
        if (col==2){
        $("#task2").append("<li class='list-group-item'>" + field + "</li> ");}
        col ++;
        
        if (col> 2){
          col=0;
          }
      });
    });
  });
</script>
